{
  "name": "vite-express",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -of .env .env.development -- nodemon src/server/main.js -w src/server",
    "prod": "vite -m production build && dotenvx run -of .env .env.production -- node src/server/main.js",
    "test": "vite -m test build && dotenvx run -of .env .env.test -- node src/server/main.js",
    "build": "vite -m production build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "element-plus": "^2.9.4",
    "express": "^4.18.2",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "mongoose": "^8.12.1",
    "multer": "1.4.5-lts.2",
    "pinia": "^3.0.1",
    "reset-css": "^5.0.2",
    "vite-express": "^0.20.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.38.4",
    "@eslint/js": "^9.20.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.1",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vue-setup-extend": "^0.4.0"
  }
}
